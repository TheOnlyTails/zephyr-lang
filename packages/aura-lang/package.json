{
	"name": "aura-lang",
	"version": "0.1.0",
	"description": "",
	"repository": {
		"type": "git",
		"url": "https://github.com/theonlytails/aura",
		"directory": "packages/aura-lang"
	},
	"license": "MIT",
	"homepage": "https://aura-lang.vercel.app/",
	"type": "module",
	"scripts": {
		"dev": "vitest",
		"build": "unbuild",
		"generate": "ohm generateBundles --withTypes 'src/**/*.ohm' --esm",
		"generate:watch": "chokidar 'src/**/*.ohm' -c 'pnpm run watch'",
		"format": "prettier -wc src/*",
		"check": "prettier -c src/*",
		"test": "vitest --run",
		"prepack": "pnpm format && pnpm build",
		"prepublishOnly": "pnpm test && pnpx publint"
	},
	"dependencies": {
		"fast-equals": "^5.0.1",
		"find-up": "^6.3.0",
		"fuzzyfind": "^2.1.0",
		"ohm-js": "^17.1.0",
		"ts-pattern": "^4.3.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@ohm-js/cli": "^2.0.0",
		"@types/node": "^20.2.5",
		"chokidar-cli": "^3.0.0",
		"unbuild": "^1.2.1",
		"vitest": "^0.32.0"
	},
	"files": [
		"/dist",
		"README.md"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./parser": {
			"types": "./dist/parser.d.ts",
			"import": "./dist/parser.mjs",
			"require": "./dist/parser.cjs"
		},
		"./interpreter": {
			"types": "./dist/interpreter.d.ts",
			"import": "./dist/interpreter.mjs",
			"require": "./dist/interpreter.cjs"
		}
	}
}
